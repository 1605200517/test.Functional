	value = data.substring(i + pos + 3, i + pos + 3 + 3);
	value = value.replaceAll(&quot;[^\\d.]&quot;, &quot;&quot;);
	//log.info(&quot;pos: &quot; + i + &quot; - &quot; + value );	
}
vars.put(&quot;attribute&quot;, value);
